<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.taskno8">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TaskNo8">
        <activity
            android:name=".DetailActivity"
            android:exported="false" />
        <activity
            android:name=".ChineeseActivity"
            android:exported="false"
            android:label="Chineese Recipe"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DesertActivity"
            android:exported="false"
            android:label="Desert Recipe"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Rice"
            android:exported="false"
            android:label="Rice Recipe"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MuttonActivity"
            android:exported="false"
            android:label="Mutton Recipe"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ChickenActivity"
            android:exported="false"
            android:label="Chicken Recipe"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".BeefActivity"
            android:exported="false"
            android:label="Beef Recipe"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>